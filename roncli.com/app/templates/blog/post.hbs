{{#ifCond post.blogSource "===" "blogger"}}
    {{partial "blog/posts/blogger"}}
{{/ifCond}}
{{#ifCond post.blogSource "===" "tumblr"}}
    {{#ifCond post.type "===" "answer"}}
        {{partial "blog/posts/tumblrAnswer"}}
    {{/ifCond}}
    {{#ifCond post.type "===" "audio"}}
        {{#ifCond post.audio_type "===" "soundcloud"}}
            {{partial "blog/posts/tumblrSoundCloud"}}
        {{/ifCond}}
        {{#ifCond post.audio_type "===" "tumblr"}}
            {{partial "blog/posts/tumblrAudio"}}
        {{/ifCond}}
    {{/ifCond}}
    {{#ifCond post.type "===" "link"}}
        {{partial "blog/posts/tumblrLink"}}
    {{/ifCond}}
    {{#ifCond post.type "===" "photo"}}
        {{partial "blog/posts/tumblrPhoto"}}
    {{/ifCond}}
    {{#ifCond post.type "===" "quote"}}
        {{partial "blog/posts/tumblrQuote"}}
    {{/ifCond}}
    {{#ifCond post.type "===" "text"}}
        {{partial "blog/posts/tumblrText"}}
    {{/ifCond}}
    {{#ifCond post.type "===" "video"}}
        {{#ifCond post.video_type "===" "youtube"}}
            {{partial "blog/posts/tumblrYouTube"}}
        {{/ifCond}}
    {{/ifCond}}
{{/ifCond}}
{{#ifCond nav "===" "true"}}
    <div>
        <div class="blog-nav blog-nav-bottom btn-group row">
            {{#if index.previous}}
                <a class="btn btn-info col-xs-3 col-md-2" href="{{{index.previous.url}}}">
                    <div><span class="glyphicon glyphicon-chevron-left"></span>&nbsp;Older</div>
                </a>
            {{else}}
                <div class="col-xs-3 col-md-2 bg-info"></div>
            {{/if}}
            <div class="col-xs-6 col-md-8 bg-info">
                <div>roncli.com Blog<br /><b>All Posts</b></div>
            </div>
            {{#if index.next}}
                <a class="btn btn-info col-xs-3 col-md-2" href="{{{index.next.url}}}">
                    <div>Newer&nbsp;<span class="glyphicon glyphicon-chevron-right"></span></div>
                </a>
            {{else}}
                <div class="col-xs-3 col-md-2 bg-info"></div>
            {{/if}}
        </div>
        <div class="blog-nav blog-nav-bottom btn-group row">
            {{#if source.previous}}
                <a class="btn btn-info col-xs-3 col-md-2" href="{{{source.previous.url}}}">
                    <div><span class="glyphicon glyphicon-chevron-left"></span>&nbsp;Older</div>
                </a>
            {{else}}
                <div class="col-xs-3 col-md-2 bg-info"></div>
            {{/if}}
            <div class="col-xs-6 col-md-8 bg-info">
                <div>Source<br/><b>{{#ifCond post.blogSource "===" "blogger"}}Blogger{{/ifCond}}{{#ifCond post.blogSource "===" "tumblr"}}Tumblr{{/ifCond}}</b></div>
            </div>
            {{#if source.next}}
                <a class="btn btn-info col-xs-3 col-md-2" href="{{{source.next.url}}}">
                    <div>Newer&nbsp;<span class="glyphicon glyphicon-chevron-right"></span></div>
                </a>
            {{else}}
                <div class="col-xs-3 col-md-2 bg-info"></div>
            {{/if}}
        </div>
        {{#each categories}}
            <div class="blog-nav blog-nav-bottom btn-group row">
                {{#if previous}}
                    <a class="btn btn-info col-xs-3 col-md-2" href="{{{previous.url}}}">
                        <div><span class="glyphicon glyphicon-chevron-left"></span>&nbsp;Older</div>
                    </a>
                {{else}}
                    <div class="col-xs-3 col-md-2 bg-info"></div>
                {{/if}}
                <div class="col-xs-6 col-md-8 bg-info">
                    <div>Category<br/><b>{{category}}</b></div>
                </div>
                {{#if next}}
                    <a class="btn btn-info col-xs-3 col-md-2" href="{{{next.url}}}">
                        <div>Newer&nbsp;<span class="glyphicon glyphicon-chevron-right"></span></div>
                    </a>
                {{else}}
                    <div class="col-xs-3 col-md-2 bg-info"></div>
                {{/if}}
            </div>
        {{/each}}
    </div>
{{/ifCond}}
